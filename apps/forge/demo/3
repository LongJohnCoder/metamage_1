#!/bin/sh

echo > v/v/text "???"

sleep 2

echo > v/v/text "Here's a simple demo:"

sleep 2

echo >> v/v/text
echo >> v/v/text "\$ cd /gui/new/port"

sleep 1.5

echo >> v/v/text "\$ ln /gui/new/caption view"

sleep 1.5

echo >> v/v/text "\$ touch window"

sleep 1.5

echo >> v/v/text "\$ echo Hello > v/text"

